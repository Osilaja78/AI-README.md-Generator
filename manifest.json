{
    "manifest_version": 3,
    "name": "AI Readme Generator",
    "version": "1.0",
    "description": "Generate README.md for GitHub repositories using AI.",
    "permissions": ["activeTab"],
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
        "16": "images/icon16.svg",
        "48": "images/icon48.svg",
        "128": "images/icon128.svg"
        }
    },
    "icons": {
        "16": "images/icon16.svg",
        "48": "images/icon48.svg",
        "128": "images/icon128.svg"
    }
}
